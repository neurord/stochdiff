<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%highlight{%d{HH:mm:ss.SSS} [%t] %level %logger{36} - %msg%n}"/>
    </Console>
  </appenders>
  <loggers>
    <root level="debug">
      <appender-ref ref="Console"/>
    </root>
  </loggers>
</configuration>
